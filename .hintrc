{
  "extends": [
    "development"
  ],
  "hints": {
    "manifest-file-extension": "off",
    "apple-touch-icons": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "button-type": "off"
  }
}